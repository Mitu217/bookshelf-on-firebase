{
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "module": "commonjs",
    "strict": true,
    "outDir": "../../dist/functions",
    "sourceMap": true,
    "target": "es2017",
    "baseUrl": "./",
    "isolatedModules": false,
    "noEmit": false,
    "esModuleInterop": true,
    "removeComments": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
